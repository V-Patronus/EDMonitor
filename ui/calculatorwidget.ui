   <string notr="true">/* ===== CALCULADORA MODERNA CON ENTRADA MANUAL ===== */

#CalculatorWidget {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1,
        stop:0 #0f172a, stop:1 #1e293b);
}

QFrame[state=&quot;true&quot;] {
	background: qlineargradient(x1:0, y1:0, x2:1, y2:1,
        stop:0 #0f172a, stop:1 #1e293b);
	border: 2px solid #55ffff;
 	border-radius: 20px;
}

QFrame[state=&quot;false&quot;] {
	border: none;
 	border-radius: 0;
}

/* ===== DISPLAY CON ENTRADA MANUAL ===== */
#displayFrame {
    background: rgba(30, 41, 59, 0.8);
    border: 2px solid rgba(59, 130, 246, 0.3);
    border-radius: 20px;
    padding: 2%  3%;
    margin: 2%;
}

#historyLabel {
    background: transparent;
    color: #94a3b8;
    font-size: clamp(10px, 2vw, 14px);
    font-weight: 400;
    border: none;
    padding: 1%;
    text-align: right;
}

#displayLineEdit {
    background: transparent;
    color: #f8fafc;
    border: none;
    font-size: 25px;
    font-weight: 300;
    font-family: 'Segoe UI', Arial, sans-serif;
    padding: 2%;
    text-align: right;
    selection-background-color: #3b82f6;
    selection-color: #ffffff;
    
    /* Cursor personalizado */
    qproperty-cursorPosition: 0;
}

#displayLineEdit:focus {
    border-radius: 12px;
    outline: none;

}

#displayLineEdit::placeholder {
    color: #64748b;
    font-style: italic;
}

/* ===== BOTONES CON BORDER RADIUS ADAPTATIVO ===== */

QPushButton[type=&quot;number&quot;] {
    background: rgba(30, 41, 59, 0.6);
    color: #e2e8f0;
    border: 1px solid rgba(59, 130, 246, 0.2);
    border-radius: 12px;
    font-size: 22px;
    min-height: 40px;
}

QPushButton[type=&quot;number&quot;]:hover {
    background: rgba(51, 65, 85, 0.7);
}



/* ===== BOTONES DE OPERACIONES ===== */
QPushButton[type=&quot;operator&quot;] {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
        stop:0 #3b82f6, stop:1 #2563eb);
    color: white;
	border: 1px solid rgba(59, 130, 246, 0.2);
    border-radius: 12px;
    min-height: 40px;
	min-width:50px
}

QPushButton[type=&quot;operator&quot;]:hover {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
        stop:0 #60a5fa, stop:1 #3b82f6);
}


/* ===== BOTÓN IGUAL DESTACADO ===== */
QPushButton#btnEquals {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
        stop:0 #10b981, stop:1 #059669);
    color: #ffffff;
   border: 1px solid rgba(59, 130, 246, 0.2);
    border-radius: 12px; 
    padding: 0;
    font-size: clamp(18px, 4vw, 32px);
    font-weight: 700;
    min-height: 40px;
}

QPushButton#btnEquals:hover {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
        stop:0 #34d399, stop:1 #10b981);
}

/* ===== BOTONES ESPECIALES ===== */
QPushButton[type=&quot;special&quot;] {
    background: #aca412;
    color: #272727;
   border: 1px solid rgba(59, 130, 246, 0.2);
    border-radius: 12px; 
    padding: 0;
    font-size: 15px;
    font-weight: bold;
    min-height: 40px; 
}

QPushButton[type=&quot;special&quot;]:hover {
    background: #e5e825;
    border-color: rgba(148, 163, 184, 0.5);
}

QPushButton:pressed {}

QPushButton[pressed=&quot;true&quot;] {
    border: 1px solid #55ffff;
}

QPushButton[pressed=&quot;false&quot;] {
    border: 1px solid rgba(59, 130, 246, 0.2);
}

QFrame[invalid=&quot;true&quot;]#displayFrame {
	border: 2px solid #ff5555;
}

QFrame[invalid=&quot;false&quot;]#displayFrame {
	border: 2px solid rgba(59, 130, 246, 0.3);
}




#lbl_Type, #lbl_digit, #Conversiones, #lblFJapon, #labelRadio {
    /* Color de texto y tipografía */
    color: #ffffff;
    font-family: &quot;Segoe UI Variable&quot;, &quot;Inter&quot;, sans-serif;
    font-size: 13px;
    font-weight: 500;

    /* Fondo: Gradiente sutil que nace del color de acento */
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, 
                                stop:0 rgba(74, 144, 226, 0.15), 
                                stop:0.6 rgba(45, 45, 45, 0.4), 
                                stop:1 transparent);

    /* Bordes redondeados y borde de acento */
    border-left: 4px solid #4a90e2; /* El resalte lateral */
    border-top-right-radius: 8px;
    border-bottom-right-radius: 8px;
    border-top-left-radius: 8px;    /* Un radio pequeño para no romper la línea vertical */
    border-bottom-left-radius: 8px;

    /* Espaciado para que no se vea apretado */
    padding: 6px 15px 6px 12px;
    margin-right: 5px;
}

/* Estilo base del QComboBox */
QComboBox[type=&quot;combo-box&quot;] {
    border: 2px solid #3d3d3d;
    border-radius: 8px;
    padding: 5px 35px 5px 15px; /* El padding derecho reserva espacio para el SVG */
    color: #ffffff;
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, 
                                stop:0 #2e2e2e, stop:1 #1a1a1a);
    selection-background-color: #4a90e2;
}

QComboBox[type=&quot;combo-box&quot;]:hover {
    border: 2px solid #5c5c5c;
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, 
                                stop:0 #383838, stop:1 #242424);
}

QComboBox[type=&quot;combo-box&quot;]:focus {
    border: 2px solid #4a90e2;
    background: #1a1a1a;
}

/* Contenedor del botón de despliegue */
QComboBox[type=&quot;combo-box&quot;]::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 30px;
    border-left: none;
}

/* LA FLECHA SVG */
QComboBox[type=&quot;combo-box&quot;]::down-arrow {
    /* Asegúrate de que la ruta sea correcta según tu archivo .qrc */
    image: url(:/iconos/iconos/chevron-down.svg); 
    width: 14px;  /* Tamaño del icono */
    height: 14px;
}

/* Efecto opcional: Cambiar la flecha cuando el menú está desplegado */
QComboBox[type=&quot;combo-box&quot;]::down-arrow:on {
    top: 1px; /* Pequeño desplazamiento al hacer click */
}

/* Flecha para estado desactivado (opcional) */
QComboBox[type=&quot;combo-box&quot;]::down-arrow:disabled {
    image: url(:/icons/chevron-down-gray.svg);
}

/* Estilo del menú desplegable (la lista) */
QComboBox[type=&quot;combo-box&quot;] QAbstractItemView {
    border: 2px solid #4a90e2;
    background-color: #1a1a1a;
    color: #ffffff;
    selection-background-color: #4a90e2;
    selection-color: #ffffff;
    outline: 0px;
    border-radius: 5px;
    padding: 5px;
}

QComboBox[type=&quot;combo-box&quot;] QAbstractItemView::item {
    min-height: 30px;
    padding-left: 10px;
    border-radius: 4px;
}

QComboBox[type=&quot;combo-box&quot;] QAbstractItemView::item:selected {
    background-color: #4a90e2;
}


/* Estilo base para todas las tarjetas (QFrame) */
#card1, #card2, #card3, #card4 {
    background-color: white;
    border-radius: 12px;
}

/* Colores específicos usando propiedades dinámicas */
QFrame[clase=&quot;azul&quot;] { border-top: 6px solid #3498db; }
QFrame[clase=&quot;rojo&quot;] { border-top: 6px solid #e74c3c; }
QFrame[clase=&quot;amarillo&quot;] { border-top: 6px solid #f1c40f; }
QFrame[clase=&quot;morado&quot;] { border-top: 6px solid #9b59b6; }



#buttonsFrame, #guiasFrame, #frameOnlyCalculator {
	 background: qlineargradient(x1:0, y1:0, x2:1, y2:1,
        stop:0 #0f172a, stop:1 #1e293b);
	border-radius:15px;
}


/* Estilo para los títulos (Labels) */
QLabel[conversor=&quot;unidad&quot;] {
	background-color: white;
    font-weight: bold;
    font-size: 14px;
    color: #2c3e50;
    margin-bottom: 5px;
}

/* Estilo para la caja de la fórmula */
#formula_1, #formula_2, 
#formula_3, #formula_4 {
    background-color: #f8f9fa;
    border: 1px solid #ddd;
    border-radius: 5px;
    padding: 8px;
    font-family: 'Courier New';
    font-weight: bold;
    color: #d35400;
}

/* Estilo para el ejemplo */
#ejemplo_1, #ejemplo_2,
#ejemplo_3, #ejemplo_4 {
	background-color: rgb(255, 255, 255);
    color: #383838;
    font-size: 12px;
	font-weight: bold;
}

/* Resalte del resultado en verde */
#resultado_1, #resultado_2,
#resultado_3, #resultado_4 {
    color: #27ae60;
    font-weight: bold;
}



/* 
   ESTILO: CYBER-GLASS UI 2025
   Enfoque: Minimalismo tecnológico, Glassmorphism y jerarquía visual.
*/

/* ---------- VENTANA PRINCIPAL ---------- */
QFrame#frameJapon {
   background: qlineargradient(x1:0, y1:0, x2:1, y2:1,
        stop:0 #0f172a, stop:1 #1e293b);
	border-radius:15px;
	font-family: &quot;Segoe UI Variable&quot;, &quot;Inter&quot;, sans-serif;
}

/* --- AHORA ES UN QLineEdit --- */
QLineEdit#lineEditRadio {
    background: rgba(255, 255, 255, 0.05);
    color: #00e5ff; /* Color cian para el número ingresado */
    border: 1px solid rgba(255, 255, 255, 0.1);
    border-radius: 15px;
    
    min-height: 40px;      /* Alto generoso estilo moderno */
    font-size: 15px;       /* Fuente grande y legible */
    font-weight: 700;
    padding-left: 15px;    /* Espacio a la izquierda */
    padding-right: 15px;   /* Espacio a la derecha */
    
    selection-background-color: #00e5ff; /* Color de selección */
    selection-color: #05050a;            /* Color del texto seleccionado */
}

QLineEdit#lineEditRadio:hover {
    border: 1px solid rgba(0, 229, 255, 0.5);
    background: rgba(255, 255, 255, 0.08);
}

QLineEdit#lineEditRadio:focus {
    border: 1px solid #00e5ff; /* Borde brillante al escribir */
    background: rgba(0, 229, 255, 0.05);
}

/* ---------- BOTÓN CALCULAR (EFECTO NEÓN) ---------- */
QPushButton#pushButtonCalcular, QPushButton#clear_fj  {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, 
                                stop:0 #246c35, stop:1 #35a24f);
    color:#eaeaea;
    font-size: 12px;
    font-weight: 900;
    border: none;
    border-radius: 14px;
    padding: 12px 30px;
    text-transform: uppercase;
    letter-spacing: 1px;
}

QPushButton#pushButtonCalcular:hover, QPushButton#clear_fj:hover {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, 
                                stop:0 #005d44, stop:1 #007352);
}

QPushButton#pushButtonCalcular:pressed, QPushButton#clear_fj:pressed {
    background: #006100;
}

/* ---------- TARJETAS (GLASSMORPHISM AVANZADO) ---------- */
QFrame[class=&quot;tarjeta-angle&quot;] {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, 
                                stop:0 rgba(255, 255, 255, 0.06), 
                                stop:1 rgba(255, 255, 255, 0.02));
    border: 1px solid rgba(255, 255, 255, 0.08);
    border-top: 1px solid rgba(255, 255, 255, 0.15); /* Brillo en el borde superior */
    border-radius: 24px;
    padding: 20px;
    margin: 10px;
}

QFrame[class=&quot;tarjeta-angle&quot;]:hover {
    background: rgba(255, 255, 255, 0.08);
    border: 1px solid rgba(0, 229, 255, 0.4);
}

/* ---------- TEXTOS DE TARJETA ---------- */

/* El nombre del ángulo (Badge Style) */
QLabel[name=&quot;number-angle&quot;] {
    color: #00e5ff;
    font-size: 15px;
    font-weight: 800;
    text-transform: uppercase;
    letter-spacing: 1.5px;
    background: rgba(0, 229, 255, 0.12);
    border-radius: 6px;
    padding: 4px 8px;
    max-height: 20px;
}

/* El valor numérico (Thin Modern Style) */
QLabel[valor=&quot;angle-c&quot;] {
    color: #ffffff;
    font-size: 38px;
    font-weight: 200;
    background: transparent;
    padding-top: 10px;
}



