   <string notr="true">/* 
   ESTILO: CYBER-GLASS UI 2025
   Enfoque: Minimalismo tecnológico, Glassmorphism y jerarquía visual.
*/

/* ---------- VENTANA PRINCIPAL ---------- */
QWidget#Form {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:1, 
                                stop:0 #05050a, stop:0.4 #0c0c1f, stop:1 #13132b);
    font-family: &quot;SF Pro Display&quot;, &quot;Inter&quot;, &quot;Segoe UI&quot;, sans-serif;
}

/* ---------- TOP BAR &amp; INPUTS ---------- */
QLabel#labelRadio {
    color: rgba(0, 229, 255, 0.8);
    font-size: 11px;
    font-weight: 800;
    text-transform: uppercase;
    letter-spacing: 2px;
    margin-bottom: 4px;
}

/* --- AHORA ES UN QLineEdit --- */
QLineEdit#lineEditRadio {
    background: rgba(255, 255, 255, 0.05);
    color: #00e5ff; /* Color cian para el número ingresado */
    border: 1px solid rgba(255, 255, 255, 0.1);
    border-radius: 15px;
    
    min-height: 40px;      /* Alto generoso estilo moderno */
    font-size: 15px;       /* Fuente grande y legible */
    font-weight: 700;
    padding-left: 15px;    /* Espacio a la izquierda */
    padding-right: 15px;   /* Espacio a la derecha */
    
    selection-background-color: #00e5ff; /* Color de selección */
    selection-color: #05050a;            /* Color del texto seleccionado */
}

QLineEdit#lineEditRadio:hover {
    border: 1px solid rgba(0, 229, 255, 0.5);
    background: rgba(255, 255, 255, 0.08);
}

QLineEdit#lineEditRadio:focus {
    border: 1px solid #00e5ff; /* Borde brillante al escribir */
    background: rgba(0, 229, 255, 0.05);
}

/* ---------- BOTÓN CALCULAR (EFECTO NEÓN) ---------- */
QPushButton#pushButtonCalcular {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, 
                                stop:0 #00f2fe, stop:1 #4facfe);
    color: #001a1a;
    font-size: 12px;
    font-weight: 900;
    border: none;
    border-radius: 14px;
    padding: 12px 30px;
    text-transform: uppercase;
    letter-spacing: 1px;
}

QPushButton#pushButtonCalcular:hover {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, 
                                stop:0 #00e5ff, stop:1 #0072ff);
}

QPushButton#pushButtonCalcular:pressed {
    background: #00c6ff;
}

/* ---------- TARJETAS (GLASSMORPHISM AVANZADO) ---------- */
QFrame[class=&quot;tarjeta-angle&quot;] {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, 
                                stop:0 rgba(255, 255, 255, 0.06), 
                                stop:1 rgba(255, 255, 255, 0.02));
    border: 1px solid rgba(255, 255, 255, 0.08);
    border-top: 1px solid rgba(255, 255, 255, 0.15); /* Brillo en el borde superior */
    border-radius: 24px;
    padding: 20px;
    margin: 10px;
}

QFrame[class=&quot;tarjeta-angle&quot;]:hover {
    background: rgba(255, 255, 255, 0.08);
    border: 1px solid rgba(0, 229, 255, 0.4);
}

/* ---------- TEXTOS DE TARJETA ---------- */

/* El nombre del ángulo (Badge Style) */
QLabel[name=&quot;number-angle&quot;] {
    color: #00e5ff;
    font-size: 10px;
    font-weight: 800;
    text-transform: uppercase;
    letter-spacing: 1.5px;
    background: rgba(0, 229, 255, 0.12);
    border-radius: 6px;
    padding: 4px 8px;
    max-height: 20px;
}

/* El valor numérico (Thin Modern Style) */
QLabel[valor=&quot;angle-c&quot;] {
    color: #ffffff;
    font-size: 38px;
    font-weight: 200;
    background: transparent;
    padding-top: 10px;
}
</string>
