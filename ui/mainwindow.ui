/* ===== BOTONES DEL GRUPO LATERAL - ESTADOS MODERNOS ===== */
#SideBarGroup QPushButton{
    background: rgba(60, 63, 65, 0.5);
    color: #bbbbbb;
    border: 1px solid rgba(85, 85, 85, 0.3);
    border-radius: 10px;
    padding: 10px 16px;
    margin: 4px 8px;
    text-align: left;
    font-weight: 500;
    font-size: 13px;
}

/* ESTADO HOVER - Elevación e iluminación */
#SideBarGroup QPushButton:hover{
    background: rgba(75, 79, 83, 0.7);
    color: #ffffff;
    border-color: rgba(119, 119, 119, 0.5);
}

/* ESTADO PRESSED - Hundido */
#SideBarGroup QPushButton:pressed{
    background: rgba(45, 48, 50, 0.8);
}

/* ESTADO ACTIVE/CHECKED - Efecto NEON */
#SideBarGroup QPushButton:checked{
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
        stop:0 #0078d4, stop:1 #00a8f4);
    color: #ffffff;
    border: 2px solid rgba(0, 168, 244, 0.5);
    font-weight: 600;
    /* Indicador lateral brillante */
    border-left: 4px solid #00f2ff;
    padding-left: 12px;
}

/* ESTADO INACTIVO/DESENFocado - Atenuado */
#SideBarGroup QPushButton:disabled{
    background: rgba(40, 43, 45, 0.3);
    color: #666666;
    border-color: rgba(85, 85, 85, 0.2);
    opacity: 0.6;
}

/* ===== ÁREA QTD-SIGNER - ESTADO ACTIVO ===== */
#SignerFrame[active=&quot;true&quot;]{
