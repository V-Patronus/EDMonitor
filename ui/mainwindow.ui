/* ===== SCROLLBAR MODERNO - GLASSMORPHISM ===== */

/* SCROLLBAR VERTICAL */
QScrollBar:vertical {
    background: transparent;
    width: 14px;
    border: none;
    margin: 16px 0 16px 0;
    border-radius: 7px;
}

QScrollBar::handle:vertical {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
        stop:0 rgba(0, 180, 216, 0.3), stop:1 rgba(0, 119, 182, 0.5));
    border: 2px solid rgba(0, 180, 216, 0.6);
    border-radius: 7px;
    min-height: 30px;
}

QScrollBar::handle:vertical:hover {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
        stop:0 rgba(0, 212, 255, 0.5), stop:1 rgba(0, 153, 204, 0.7));
    border-color: rgba(0, 212, 255, 0.8);
}

QScrollBar::handle:vertical:pressed {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
        stop:0 rgba(0, 153, 204, 0.6), stop:1 rgba(0, 119, 182, 0.8));
}

/* Flechas del scrollbar - estilizadas */
QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    background: rgba(0, 180, 216, 0.2);
    border: 1px solid rgba(0, 180, 216, 0.4);
    border-radius: 7px;
    height: 14px;
    subcontrol-origin: margin;
}

QScrollBar::add-line:vertical {
    subcontrol-position: bottom;
    margin-bottom: 2px;
}

QScrollBar::sub-line:vertical {
    subcontrol-position: top;
    margin-top: 2px;
}

QScrollBar::add-line:vertical:hover, QScrollBar::sub-line:vertical:hover {
    background: rgba(0, 212, 255, 0.3);
    border-color: rgba(0, 212, 255, 0.6);
}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background: transparent;
}

/* SCROLLBAR HORIZONTAL */
QScrollBar:horizontal {
    background: transparent;
    height: 14px;
    border: none;
    margin: 0 16px 0 16px;
    border-radius: 7px;
}

QScrollBar::handle:horizontal {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
        stop:0 rgba(0, 180, 216, 0.3), stop:1 rgba(0, 119, 182, 0.5));
    border: 2px solid rgba(0, 180, 216, 0.6);
    border-radius: 7px;
    min-width: 30px;    
}

QScrollBar::handle:horizontal:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
        stop:0 rgba(0, 212, 255, 0.5), stop:1 rgba(0, 153, 204, 0.7));
    border-color: rgba(0, 212, 255, 0.8);    
}

QScrollBar::handle:horizontal:pressed {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
        stop:0 rgba(0, 153, 204, 0.6), stop:1 rgba(0, 119, 182, 0.8));
}

QScrollBar::add-line:horizontal, QScrollBar::sub-line:horizontal {
    background: rgba(0, 180, 216, 0.2);
    border: 1px solid rgba(0, 180, 216, 0.4);
    border-radius: 7px;
    width: 14px;
    subcontrol-origin: margin;
}

QScrollBar::add-line:horizontal {
    subcontrol-position: right;
    margin-right: 2px;
}

QScrollBar::sub-line:horizontal {
    subcontrol-position: left;
    margin-left: 2px;
}

QScrollBar::add-line:horizontal:hover, QScrollBar::sub-line:horizontal:hover {
    background: rgba(0, 212, 255, 0.3);
    border-color: rgba(0, 212, 255, 0.6);
}

QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
    background: transparent;
}

/* CORNER (esquina entre scrollbars) */
QScrollBar::corner {
    background: transparent;
}


