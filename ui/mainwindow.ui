                          <property name="text">
                           <string>¿Qué es Wire EDM?</string>
                          </property>
                          <property name="textFormat">
                           <enum>Qt::TextFormat::PlainText</enum>
                          </property>
                          <property name="alignment">
                           <set>Qt::AlignmentFlag::AlignCenter</set>
                          </property>
